{
	"name": "docs-project",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"transform-docs": "ts-node scripts/transformDocs.ts && python3 scripts/process_autodoc.py",
		"fetch-options": "ts-node scripts/fetchOptions.ts",
		"update-submodules": "git submodule update --init --recursive --remote external-repos/console-docs && git submodule update --init --recursive --remote external-repos/netbox",
		"dev": "yarn update-submodules && yarn fetch-options && yarn transform-docs && docusaurus start --port 3001",
		"docusaurus": "docusaurus",
		"start": "docusaurus start",
		"build": "yarn update-submodules && yarn fetch-options && yarn transform-docs && docusaurus build",
		"swizzle": "docusaurus swizzle",
		"deploy": "docusaurus deploy",
		"clear": "docusaurus clear",
		"serve": "docusaurus serve",
		"write-translations": "docusaurus write-translations",
		"write-heading-ids": "docusaurus write-heading-ids",
		"typecheck": "tsc",
		"postinstall": "git submodule update --init --recursive",
		"process-autodoc": "python3 scripts/process_autodoc.py"
	},
	"dependencies": {
		"@docusaurus/core": "^3.8.0",
		"@docusaurus/plugin-content-docs": "^3.8.0",
		"@docusaurus/preset-classic": "^3.8.0",
		"@docusaurus/theme-mermaid": "^3.8.0",
		"@mdx-js/react": "^3.0.0",
		"@uidotdev/usehooks": "^2.4.1",
		"clsx": "^2.0.0",
		"docusaurus-plugin-remote-content": "^4.0.0",
		"motion": "^12.10.0",
		"prism-react-renderer": "^2.3.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"@algolia/client-search": "^4.9.1",
		"@docusaurus/module-type-aliases": "^3.8.0",
		"@docusaurus/tsconfig": "^3.8.0",
		"@docusaurus/types": "^3.8.0",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^18.0.0",
		"@types/react": "^18.0.0",
		"@types/react-loadable": "^5.5.0",
		"acorn": "^8.0.0",
		"autoprefixer": "^10.4.21",
		"docusaurus-plugin-sass": "^0.2.6",
		"js-yaml": "^4.1.0",
		"postcss": "^8.5.3",
		"react-loadable": "^5.5.0",
		"sass": "^1.87.0",
		"search-insights": "^2.0.0",
		"tailwindcss": "3",
		"ts-node": "^10.9.2",
		"typescript": "~5.6.2"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 3 chrome version",
			"last 3 firefox version",
			"last 5 safari version"
		]
	},
	"engines": {
		"node": ">=22.0.0"
	},
	"resolutions": {
		"webpack-dev-server": ">=5.2.1"
	}
}
